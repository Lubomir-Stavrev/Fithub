{
  "expo": {
    "name": "FitHub",
    "slug": "FitHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fitLogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fit.ap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fitLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "fit.ap",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7464607351320039~6076176382"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
